# Copy to .env and fill values as needed.
#
# Notes:
# - Many scripts will auto-load `.env`.
# - `scan-type4-authorities.ts`/`.mjs` can disable that via `SKIP_DOTENV=true`.

########################################
# Hypersync (shared)
########################################

# Required for Hypersync-backed scans
HYPERSYNC_API_KEY=""

# Optional: chain slug used to build `https://${CHAIN}.hypersync.xyz` (default: base)
CHAIN="base"

# Optional: full Hypersync URL override (takes precedence over CHAIN)
# HYPERSYNC_URL="https://base.hypersync.xyz"

# Optional output prefix for filenames. If unset and CHAIN is unset and chain is base, files are unprefixed.
# OUTPUT_PREFIX="base"

# Optional: transaction type to scan (default: 4)
# TXN_TYPE="4"

########################################
# scan.ts (targeted scan + AddOwner bucketing)
########################################

# Optional: start block (default: 13514406)
# FROM_BLOCK="13514406"

# Optional: comma-separated list of delegate target addresses to match in authorization tuples
# TARGET_ADDRESSES="0x...,0x..."

# Optional: reuse existing from-addresses file instead of fetching transactions again
# SKIP_FETCH_TRANSACTIONS="false"

########################################
# scan-type4-authorities.ts / scan-type4-authorities.mjs
########################################

# Optional: disable auto-loading `.env` (recommended for one-off shell overrides)
# SKIP_DOTENV="false"

# Optional: end block (exclusive). Set empty to scan to tip.
# TO_BLOCK=""

# Optional: resume from the state file (type4-scan-state.json) written during scans
# RESUME="false"

# Optional: print a few authorizationList[0] samples then exit
# DEBUG_SAMPLE="false"
# DEBUG_SAMPLE_LIMIT="3"

# Optional: Hypersync stream tuning (can help with rate limits / timeouts)
# HYPERSYNC_STREAM_CONCURRENCY="1"
# HYPERSYNC_BATCH_SIZE="5000"
# HYPERSYNC_MAX_BATCH_SIZE="20000"

# Optional: when using paginated get() mode (see USE_PAGINATED_GET)
# HYPERSYNC_REQUEST_DELAY_MS="0"
# VERBOSE_PAGINATION="false"

# Optional: use `hypersyncClient.get()` pagination instead of streaming
# USE_PAGINATED_GET="false"

# Optional: retries for fetching Hypersync height (used to clamp TO_BLOCK safely)
# HYPERSYNC_HEIGHT_RETRIES="5"
# HYPERSYNC_HEIGHT_RETRY_DELAY_MS="250"

########################################
# market-share.ts (RPC state scan)
########################################

# Required: RPC URL for the selected CHAIN, using RPC_URL_<CHAIN> convention
# Examples:
RPC_URL_BASE=""
# RPC_URL_OPTIMISM=""
# RPC_URL_ARBITRUM=""

# Optional: override RPC URL when CHAIN matches; otherwise prefer RPC_URL_<CHAIN>
# RPC_URL=""

# Optional: override the input authorities file path
# AUTHORITIES_FILE=""

# Optional: output directory (default: .)
# OUTPUT_DIR="."

# Optional: concurrency for parallel eth_getCode calls (default: 16)
# CONCURRENCY="16"

# Optional: chunk size and progress logging
# CHUNK_SIZE="25000"
# PROGRESS_EVERY_MS="10000"

# Optional: resume from checkpoint file
# RESUME="false"
# CHECKPOINT_FILE=""

# Optional: provider retry/backoff behavior
# RPC_MAX_RETRIES="8"
# RPC_REQUEST_DELAY_MS="0"
# RPC_RETRY_BASE_DELAY_MS="250"
# RPC_RETRY_MAX_DELAY_MS="10000"

# Optional: highlight Coinbase's delegate in outputs (0x-prefixed 20-byte hex)
# COINBASE_DELEGATE_ADDRESS="0x..."

########################################
# render-report.ts (HTML report)
########################################

# Required: input JSON path (output of market-share.ts)
# INPUT_JSON="results/<timestamp>/market-share/base-delegate-market-share.json"

# Optional: report output location
# OUTPUT_DIR="."
# OUTPUT_FILE="delegate-market-share-report.html"

# Optional: report sizing knobs
# TOP_N="15"
# TABLE_N="30"
# PARETO_POINTS="500"

########################################
# aggregate-labels.ts (delegate -> wallet/provider grouping)
########################################

# Required: input JSON path (output of market-share.ts)
# INPUT_JSON="results/<timestamp>/market-share/base-delegate-market-share.json"

# Optional: labels mapping and output
# LABELS_JSON="labels/wintermute-custom-labels.json"
# OUTPUT_DIR="."

# Optional: how to treat unknown delegates: "group" | "keep"
# UNLABELED_MODE="group"

# Optional: override output basename (without extension)
# OUTPUT_BASENAME=""
